<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RecyclerViewBanner">
        <attr name="interval" format="integer"/>
        <attr name="showIndicator" format="boolean"/>
        <attr name="indicatorSelectedSrc" format="color|reference"/>
        <attr name="indicatorUnselectedSrc" format="color|reference"/>
        <attr name="indicatorSpace" format="dimension|reference"/>
        <attr name="indicatorMarginLeft" format="dimension|reference"/>
        <attr name="indicatorMarginRight" format="dimension|reference"/>
        <attr name="indicatorMarginBottom" format="dimension|reference"/>
        <attr name="indicatorGravity" format="enum">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="autoPlaying" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="RecyclerViewBannerNew">
        <attr name="intervalN" format="integer"/>
        <attr name="showIndicatorN" format="boolean"/>
        <attr name="indicatorSelectedSrcN" format="color|reference"/>
        <attr name="indicatorUnselectedSrcN" format="color|reference"/>
        <attr name="indicatorSpaceN" format="dimension|reference"/>
        <attr name="indicatorMarginLeftN" format="dimension|reference"/>
        <attr name="indicatorMarginRightN" format="dimension|reference"/>
        <attr name="indicatorMarginBottomN" format="dimension|reference"/>
        <attr name="indicatorGravityN" format="enum">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
        <attr name="orientationN" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="autoPlayingN" format="boolean"/>
    </declare-styleable>


</resources>